game:GetService("UserInputService").InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.B then
		script.Parent.Visible = not script.Parent.Visible
	end
end)

for i, v in pairs(script.Parent.BSections_Frame.Sections_ScrollingFrame:GetChildren()) do
	if v:IsA("TextButton") then
		v.MouseButton1Click:Connect(function()
			script.Parent.Assets.button.Playing = true
		end)
	end
end

local player = game:GetService("Players").LocalPlayer
local imagePlayer = "http://www.roblox.com/Thumbs/Avatar.ashx?x=150&y=150&Format=Png&username=" .. player.Name

script.Parent.Sections.Sections_Home.avatar.Image = imagePlayer

local delay = 10

local function notif(str)
	game:GetService("StarterGui"):SetCore("SendNotification", {
		Title = "Nikitka~",
		Text = str,
		Icon = "rbxassetid://13376382479",
		Duration = delay
	})
end

notif("Welcome to myselfjj! by Nikitka!~")

	local TARGET_txb = script.Parent.Sections.Sections_Target.TARGET_textbox
	local TARGET_txt = script.Parent.Sections.Sections_Target.TARGET_textbox.Text
	local TARGET_avt = script.Parent.Sections.Sections_Target.Tavatar
	local TARGET_Name = script.Parent.Sections.Sections_Target.Info.NAME
	local TARGET_DName = script.Parent.Sections.Sections_Target.Info.DNAME
	local TARGET_UserID = script.Parent.Sections.Sections_Target.Info.USERID
	local GetIt = script.Parent.Sections.Sections_Target.GetIt
	GetIt.MouseButton1Click:Connect(function()
		if game:GetService("Players"):FindFirstChild(TARGET_txt) then
			local imageTarget = "http://www.roblox.com/Thumbs/Avatar.ashx?x=150&y=150&Format=Png&username=" .. TARGET_txt.Name
			TARGET_avt.Image = imageTarget
			TARGET_Name.Text = TARGET_txt
			TARGET_DName.Text = TARGET_txt.DisplayName
			TARGET_UserID.Text = TARGET_txt.UserId
		end
	end)
